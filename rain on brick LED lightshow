// C++ code
// eighth note: 340.91
// quarter note: 681.82
// dotted quarter note: 1022.7
// half note: 1363.63
// dotted half note: 1704.54


void setup()
{
  for(int i = 2; i < 8 ; i++) {
    pinMode(i, OUTPUT);
  }
  
  digitalWrite(2, 1);
  delay(340.91);
  
  digitalWrite(2, 0);
  digitalWrite(3, 1);
  delay(340.91);
  
  //
  
  digitalWrite(3, 0);
  digitalWrite(4, 1);
  digitalWrite(5, 1);
  delay(681.82);
  
  digitalWrite(4, 0);
  digitalWrite(5, 0);
  digitalWrite(2, 1);
  digitalWrite(7, 1);
  delay(681.82);
  
