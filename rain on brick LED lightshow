  delay(340.91);
  
  //
  digitalWrite(3, 0);
  digitalWrite(4, 1);
  delay(340.91);
  
  digitalWrite(4, 0);
  digitalWrite(7, 1);
  delay(340.91);
  
  digitalWrite(7, 0);
  digitalWrite(5, 1);
  digitalWrite(6, 1);
  delay(681.82);
  
  digitalWrite(5, 0);
